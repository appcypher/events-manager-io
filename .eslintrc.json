{
    "extends": "airbnb",
    "globals": {
      "expect": true,
      "beforeAll": true,
      "afterAll": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]
      }],
      "react/prop-types": 0,
      "react/no-unused-state": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "import/extensions": 0,
      "function-paren-newline": 0
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "mocha": true
    },
    "parser": "babel-eslint"
}
